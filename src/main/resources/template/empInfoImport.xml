<?xml version="1.0" encoding="UTF-8"?>
<workbook>
    <worksheet idx="0">
        <section startRow="0" endRow="0"></section>
        <loop startRow="1" endRow="1" items="empInfoList" var="empInfo" varType="com.graduation.oa.data.EmpInfo">
            <section startRow="1" endRow="1">
                <mapping row="1" col="0">empInfo.name</mapping>
                <mapping row="1" col="1">empInfo.deptName</mapping>
                <mapping row="1" col="2">empInfo.grade</mapping>
                <mapping row="1" col="3">empInfo.postName</mapping>
                <mapping row="1" col="4">empInfo.status</mapping>
                <mapping row="1" col="5">empInfo.needScore</mapping>
            </section>
            <loopbreakcondition>
                <rowcheck offset="0">
                    <cellcheck offset="0"></cellcheck>
                    <cellcheck offset="1"></cellcheck>
                    <cellcheck offset="2"></cellcheck>
                    <cellcheck offset="3"></cellcheck>
                    <cellcheck offset="4"></cellcheck>
                    <cellcheck offset="5"></cellcheck>
                </rowcheck>
            </loopbreakcondition>
        </loop>
    </worksheet>
</workbook>