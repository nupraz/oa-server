<?xml version="1.0" encoding="UTF-8"?>
<workbook>
    <worksheet idx="0">
        <section startRow="0" endRow="0"></section>
        <loop startRow="1" endRow="1" items="recordList" var="deptAssRecord" varType="com.graduation.oa.data.DeptAssRecord">
            <section startRow="1" endRow="1">
                <mapping row="1" col="0">deptAssRecord.assessMonth</mapping>
                <mapping row="1" col="1">deptAssRecord.depName</mapping>
                <mapping row="1" col="2">deptAssRecord.taskPerScore</mapping>
                <mapping row="1" col="3">deptAssRecord.jurName</mapping>
                <mapping row="1" col="4">deptAssRecord.tmpCooperateScore</mapping>
                <mapping row="1" col="5">deptAssRecord.cooperateRemark</mapping>
                <mapping row="1" col="6">deptAssRecord.manageTime</mapping>
            </section>
            <loopbreakcondition>
                <rowcheck offset="0">
                    <cellcheck offset="0"></cellcheck>
                    <cellcheck offset="1"></cellcheck>
                    <cellcheck offset="2"></cellcheck>
                    <cellcheck offset="3"></cellcheck>
                    <cellcheck offset="4"></cellcheck>
                    <cellcheck offset="5"></cellcheck>
                    <cellcheck offset="6"></cellcheck>
                </rowcheck>
            </loopbreakcondition>
        </loop>
    </worksheet>
</workbook>